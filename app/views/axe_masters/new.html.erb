<%= simple_form_for(@axe_master, :html => {:class => 'form-horizontal' }) do |f| %>
	<%= f.input :seq_no %>
	<%= f.hidden_field :user_id, :value => current_user.email %>
	<%= f.input :museam_ref %>
	<%= f.input :townland %>
	<%= f.input :county %>
	<%= f.input :profile_id, :as => :select, :collection => Hash[Profile.all.map{|b| [b.description,b.code]}] %>
	<%= f.submit %>
<% end %>
 